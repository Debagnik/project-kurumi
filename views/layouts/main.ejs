    <head>
        <meta charset="utf-8"/>
        <meta http-equiv="content-script-type" content="text/html"/>
        <meta http-equiv="content-script-type" content="text/javascript"/>
        <meta http-equiv="content-script-type" content="text/css"/>
        <meta http-equiv="content-type" content="text/html; charset=UTF-8"/>
        <meta name="keywords" content="<%= [locals.config.siteMetaDataKeywords, locals.keywords, locals.title].filter(Boolean).join(', ') || 'Project Walnut' %>"/>
        <meta name="description" content="<%= [locals.config.siteMetaDataDescription, locals.description].filter(Boolean).join(', ') || 'A random description as a placeholder' %>"/>
        <meta name="author" content="<%= (locals.config.siteMetaDataAuthor ? `${locals.config.siteMetaDataAuthor}` : 'Debagnik Kar' )%>"/>
        <%- locals.config.googleAnalyticsScript ? locals.config.googleAnalyticsScript.trim() : '' %>
        <%- locals.config.inspectletScript ? locals.config.inspectletScript.trim() : '' %>
        <meta http-equiv="x-ua-compatible" content="ie=edge"/>
        <meta name="viewport" content="width=device-width, initial-scale=1"/>
        <meta name="robots" content="index, follow">
        <meta name="revisit-after" content="1 days">
        <meta name="msapplication-TileColor" content="#da532c">
        <meta name="theme-color" content="#000000">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <link rel="stylesheet" href="/css/style.css">
        <script type="text/javascript" defer src="/js/script.js"></script>
        <title><%= locals.title %></title>
        <link rel="icon" sizes="32x32" href="img\favicon_32x32.png" type="image/png">
        <link rel="icon" sizes="16x16" href="img\favicon_16x16.png" type="image/png">
        <link rel="icon" sizes="768X768" href="img\favicon.png" type="image/png">
    </head>
<body>
    <%- include('../partials/search.ejs') %>
        <div class="container">
            <%- include('../partials/header.ejs') %>
                <main class="main">
                    <%- include('../partials/flash') %>
                    <%- body %>
                </main>
                <%- include('../partials/footer.ejs') %>
        </div>
    <script src="/js/theme.js"></script>
</body>

</html>